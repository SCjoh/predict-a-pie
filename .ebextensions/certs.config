container_commands: 
  restart_nginx:
    command: "service nginx restart" 
  01-build:
    command: "npm run build"
  02-copy-key:
    command: "aws s3 cp s3://site-ssl-certificates/nn-staging_inventor_city_PK.key /etc/pki/tls/certs/nn-staging_inventor_city_PK.key"
  02-copy-ssl-chain:
    command: "aws s3 cp s3://site-ssl-certificates/nn-staging_inventor_city_chain.crt /etc/pki/tls/certs/nn-staging_inventor_city_chain.crt"      